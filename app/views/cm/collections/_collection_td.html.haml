%td= link_to collection.name, collection
%td= collection_process_tag collection
%td
  = collection.percent
  \%
%td.red
  = number_to_currency((collection.contract.amount/100*collection.percent).round(2), unit: "")
  元
%td= collection.time.strftime "%Y-%m-%d"
%td
  = link_to '编辑', edit_cm_collection_path(collection), remote: true, class: "btn btn-warning btn-xs"
  = link_to '删除', collection, method: :delete, data: { confirm: '确定删除?' }, remote: true, class: "btn btn-danger btn-xs"
