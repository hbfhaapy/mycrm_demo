= form_for @cm_collection do |f|
  - if @cm_collection.errors.any?
    #error_explanation
      %h2= "#{pluralize(@cm_collection.errors.count, "error")} prohibited this cm_collection from being saved:"
      %ul
        - @cm_collection.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :percent
    = f.datetime_select :percent
  .field
    = f.label :time
    = f.datetime_select :time
  .field
    = f.label :contact_id
    = f.number_field :contact_id
  .field
    = f.label :user_id
    = f.text_field :user_id
  .actions
    = f.submit 'Save'
